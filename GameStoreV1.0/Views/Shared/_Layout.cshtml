    <header>
         @if (isCookieExists3)
    {
        @Html.Partial("Header3")
    }
    else
    {
        if (isCookieExists2)
        {
           @Html.Partial("Header2")
        }
        else
        {
            if (isCookieExists)
            {
                @Html.Partial("Header1")
            }
            else {@Html.Partial("Header") }
        }
    }
       
