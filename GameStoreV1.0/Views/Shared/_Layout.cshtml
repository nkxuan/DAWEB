@{
    bool isCookieExists = Context.Request.Cookies.ContainsKey("UserCookie");
    bool isCookieExists2 = Context.Request.Cookies.ContainsKey("SellerCookie");
    bool isCookieExists3 = Context.Request.Cookies.ContainsKey("AdminCookie");
}
